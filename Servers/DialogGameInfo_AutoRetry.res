	layout
	{
		//top
		region { name=top margin-bottom=28 }
		region { name=left region=top x=0 y=30 margin-left=10 width=95 }
		region { name=right region=top x=0 y=30 margin-right=10 width=306 align=right }

		//left
		place { region=left control=ServerIPLabel x=0 y=3 align=right }
		place { region=left control=ServerLabel.GameLabel,MapLabel.PlayersLabel,Label1,PingLabel y=18 align=right spacing=2 dir=down }
		
		//right
		place { region=right control=ServerIPText height=19 }
		place { region=right control=ServerText,GameText,MapText,PlayersText,SecureText,PingText x=4 y=18 width=max spacing=2 dir=down }
		

		//Content
		region { name=container region=top x=0 y=138 margin-left=2 margin-right=2 }
		place { region=container control="PlayerList" x=0 y=0 width=max height=max dir=down margin-bottom=62 }
		place { region=container start=PlayerList control="InfoLabel" width=max height=18 y=0 x=8 dir=down }
		place { region=container start=InfoLabel control="AutoRetryAlert,AutoRetryJoin" width=max height=20 x=0 y=0 dir=down }
	
		//Bottom
		region { name=bottom align=bottom height=24 }
		place {	control="AutoRetry,Connect,Refresh,Close" region=bottom align=right spacing=4 height=20 width=84 margin-bottom=4 margin-right=4 }		
	}
